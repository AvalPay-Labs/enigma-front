# Network selection: "fuji" or "mainnet"
VITE_NETWORK=fuji

# RPC URLs (optional; defaults from chain config if empty)
VITE_FUJI_RPC_URL=
VITE_MAINNET_RPC_URL=

# Optional: EERC contract address overrides
VITE_EERC_CONTRACT_ADDRESS=

# Circuit URLs (set a base or individual URLs for each circuit)
# If not set, the app will look under "/circuits/<name>/<name>.wasm|.zkey"
VITE_EERC_CIRCUIT_BASE_URL=
# VITE_CIRCUIT_REGISTER_WASM_URL=
# VITE_CIRCUIT_REGISTER_ZKEY_URL=
# VITE_CIRCUIT_TRANSFER_WASM_URL=
# VITE_CIRCUIT_TRANSFER_ZKEY_URL=
# VITE_CIRCUIT_MINT_WASM_URL=
# VITE_CIRCUIT_MINT_ZKEY_URL=
# VITE_CIRCUIT_WITHDRAW_WASM_URL=
# VITE_CIRCUIT_WITHDRAW_ZKEY_URL=
# VITE_CIRCUIT_BURN_WASM_URL=
# VITE_CIRCUIT_BURN_ZKEY_URL=

# Mock mode for local development without circuits (1 to enable)
# TODO: Set to 0 (or remove) in production.
VITE_EERC_MOCK=1
